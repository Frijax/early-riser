# News API Configuration
VITE_NEWS_API_KEY=your_news_api_key_here

# Market Data API
VITE_MARKET_DATA_API_KEY=your_market_data_key_here
VITE_MARKET_DATA_BASE_URL=https://api.marketdata.com/v1

# Cryptocurrency Data
VITE_CRYPTO_API_KEY=your_crypto_api_key_here

# WebSocket Configuration
VITE_WS_URL=wss://stream.marketdata.com

# Application Settings
VITE_ENVIRONMENT=development
VITE_LOG_LEVEL=info

# Feature Flags
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=false
VITE_ENABLE_AI_INSIGHTS=true

# Update Intervals (in milliseconds)
VITE_NEWS_REFRESH_INTERVAL=60000
VITE_PRICE_REFRESH_INTERVAL=30000